const int sensor1=A0; //se declara la variable para hacer referencia a que el sensor est치 conectado en Pin 0
const int sensor2=A1;
 

void setup()

{

Serial.begin(9600); // Inicializa el serial

}

 

void loop()

{

int val1= analogRead(sensor1); // lee la se침al de entrada del sensor y lo almacena en la variable val
int val2=analogRead(sensor2);

if(val1>=50){
  Serial.println(val1); // Imprime el valor de val en cada rengl칩n
  delay(200);
}

if(val2>=50){
 val2=val2+1025;
  Serial.println(val2); // Imprime el valor de val en cada rengl칩n
  delay(200);
}


delay(200);

}
